    next
end
config system switch-interface
    edit "MGMT-SVI"
        set vdom "root"
        set member "MGMT-ACCESS" "MGMT-TRUNK"
    next
end
config system interface
    edit "ha"
        set vdom "root"
        set allowaccess ping fgfm
        set status down
        set type physical
        set snmp-index 1
    next
    edit "mgmt"
        set vdom "root"
        set ip 192.168.1.99 255.255.255.0
        set allowaccess ping https ssh http fgfm
        set type physical
        set dedicated-to management
        set role lan
        set snmp-index 2
    next
    edit "wan1"
        set vdom "root"
        set mode dhcp
        set allowaccess ping https ssh http fgfm
        set status down
        set type physical
        set role wan
        set snmp-index 3
    next
    edit "wan2"
        set vdom "root"
        set mode dhcp
        set allowaccess ping https ssh http fgfm
        set status down
        set type physical
        set role wan
        set snmp-index 4
    next
    edit "dmz1"
        set vdom "root"
        set type physical
        set snmp-index 5
    next
    edit "dmz2"
        set vdom "root"
        set type physical
        set role dmz
        set snmp-index 6
    next
    edit "modem"
        set vdom "root"
        set mode pppoe
        set type physical
        set snmp-index 9
    next
    edit "ssl.root"
        set vdom "root"
        set type tunnel
        set alias "SSL VPN interface"
        set snmp-index 10
    next
    edit "SWITCH-AGG"
        set vdom "root"
        set type aggregate
        set member "dmz1" "dmz2"
        set device-identification enable
        set role lan
        set snmp-index 7
    next
{% for vlan in site_vlans %}
{% if vlan == "MGMT" %}
    edit "{{ vlan }}-SVI"
        set vdom "root"
        set ip {{ site_vlans[vlan]['ve_ip'] }} {{ site_vlans[vlan]['ve_mask'] }}
        set allowaccess ping https ssh http fgfm
        set type switch
        set role lan
        set snmp-index {{ loop.index+10 }}
        {% if site_vlans[vlan]['ve_helper1'] is defined %}set dhcp-relay-service enable
        set dhcp-relay-ip "{{ site_vlans[vlan]['ve_helper1'] }}"
    {% endif %}next
    edit "{{ vlan }}-TRUNK"
        set vdom "root"
        set role lan
        set snmp-index {{ loop.index+11 }}
        set interface "SWITCH-AGG"
        set vlanid {{ site_vlans[vlan]['vlan_id'] }}
    next
    edit "{{ vlan }}-ACCESS"
        set vdom "root"
        set type hard-switch
        set stp enable
        set role lan
        set snmp-index {{ loop.index+12 }}
    next    {% else %}    edit "{{ vlan }}{% if vlan == 'GUEST'%}S{% endif %}"
        set vdom "root"
        {% if site_vlans[vlan]['ve_ip'] is defined%}set ip {{ site_vlans[vlan]['ve_ip'] }}{% if site_vlans[vlan]['ve_mask'] is defined %} {{ site_vlans[vlan]['ve_mask'] }}
        {% endif %}{% endif %}set allowaccess ping https ssh http {% if site_vlans[vlan]['WAN'] is defined %}{% if site_vlans[vlan]['WAN'] == True %}fgfm{% endif %}{% elif site_vlans[vlan]['EPLAN'] is defined %}{% if site_vlans[vlan]['EPLAN'] == True %}fgfm{% endif %}{% endif %}

        {% if site_vlans[vlan]['WAN'] is defined %}{% if site_vlans[vlan]['WAN'] == True %}set role wan
        {% endif %}{% elif vlan == 'GUEST' %}{% else %}set role lan
        {% endif %}set snmp-index {{ loop.index+10 }}
        {% if site_vlans[vlan]['ve_helper1'] is defined %}set dhcp-relay-service enable
        set dhcp-relay-ip "{{ site_vlans[vlan]['ve_helper1'] }}"
        {% endif %}set interface "SWITCH-AGG"
        set vlanid {{ site_vlans[vlan]['vlan_id'] }}
    next{% endif %}

{% endfor %}
end
